{
  "manifest_version": 2,
  "name": "Goto",
  "description": "Select a text and go to the specified URL",
  "version": "1.3.1",
  "icons": {
    "16": "img/16x16.png",
    "48": "img/48x48.png",
    "128": "img/128x128.png"
  },
  "permissions": [
    "contextMenus",
    "downloads",
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": ["js/common.js", "js/event-page.js"]
  },
  "options_ui": {
    "chrome_style": true,
    "page": "html/options.html"
  }
}
